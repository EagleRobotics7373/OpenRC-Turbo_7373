            <LynxColorSensor name="mgcs2" bus="2" port="0"/>
            <LynxColorSensor name="mgcs3" bus="3" port="0"/>
        </LynxModule>
        <LynxModule name="Expansion Hub 1" port="1">
            <Matrix12vMotor name="backRightMotor" port="3" />
